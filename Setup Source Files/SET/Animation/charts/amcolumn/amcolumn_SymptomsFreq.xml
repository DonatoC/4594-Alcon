<settings>
<data_type>csv</data_type>
<decimals_separator>.</decimals_separator>
  <background>
    <alpha>100</alpha>
    <border_alpha>20</border_alpha>
  </background>
  <grid>
    <category>
      <dashed>1</dashed>
    </category>
    <value>
      <dashed>1</dashed>
    </value>
  </grid>
  <axes>
    <category>
      <width>1</width>
      <color>E7E7E7</color>
    </category>
    <value>
      <width>1</width>
      <color>E7E7E7</color>
    </value>
  </axes>
  <values>
    <value>
      <min>0</min>
      <unit>%</unit>
    </value>
  </values>
  <depth>15</depth>
  <legend>
    <text_size>18</text_size>
  </legend>
  <column>
    <width>85</width>
    <balloon_text>{title}: {value} downloads</balloon_text>
    <data_labels>{value} %</data_labels>
    <data_labels_text_color>FFFFFF</data_labels_text_color>
    <data_labels_text_size>14</data_labels_text_size>
    <data_labels_position>above</data_labels_position>
  </column>
  <graphs>
    <graph gid="0">
      <title>Floaters</title>
      <color>E58F36</color>
    </graph>
    <graph gid="1">
      <title>Flashes Of Light</title>
      <color>2981AB</color>
    </graph>
    <graph gid="2">
      <title>Distorted Vision</title>
      <color>C4D542</color>
    </graph>
    <graph gid="3">
      <title>Objects Appear Smaller</title>
      <color>707070</color>
    </graph>
    <graph gid="4">
      <title>Central Black Image</title>
      <color>C18136</color>
    </graph>
    <graph gid="5">
      <title>Difficulty Reading at Night</title>
      <color>2D8699</color>
    </graph>
    <graph gid="6">
      <title>Blurred Vision</title>
      <color>4EA748</color>
    </graph>
    <graph gid="7">
      <title>Visual Reduction</title>
      <color>595959</color>
    </graph>
    <graph gid="8">
      <title>Other</title>
      <color>8D5F29</color>
    </graph>
    <graph gid="9">
      <title>Not relevant</title>
      <color>40B1C5</color>
    </graph>
  </graphs>
  <labels>
    <label lid="0">
      <text><![CDATA[<b>Comorbidities: - Frequency</b>]]></text>
      <text_color>2981AB</text_color>
      <text_size>32</text_size>
      <align>center</align>
    </label>
    <label lid="1">
      <text>Frequency of ocular symptoms</text>
      <y>50%</y>
      <text_size>12</text_size>
      <rotate>1</rotate>
    </label>
  </labels>
</settings>